@model List<CheeseMVC.Models.CheeseCategory>

<h1>@ViewBag.Title</h1>



@foreach (var category in Model)
{
<ul class="list-group">
    <li class="list-group-item">@category.Name</li>
    
</ul>

}


@if (Model.Count == 0)
{
    <p>No categories yet!</p>
}

<p><a asp-controller="Category" asp-action="Add">Add Category</a></p>
